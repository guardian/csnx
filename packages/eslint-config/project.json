{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/eslint-config",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@csnx/npm-package:build",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/eslint-config",
				"packageJson": "packages/eslint-config/package.json",
				"assets": [
					"packages/eslint-config/.eslintrc.js",
					"packages/eslint-config/*.md"
				]
			}
		}
	},
	"tags": []
}
