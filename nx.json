{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		".eslintrc.json": "*",
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		}
	},
	"npmScope": "guardian",
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"]
		}
	},
	"tasksRunnerOptions": {
		"default": {
			"options": {
				"accessToken": "OTdkYmFjMTYtNGFhNS00MjM2LTg0OGMtZmNhNGZiMmU2YTM5fHJlYWQtd3JpdGU=",
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			},
			"runner": "@nrwl/nx-cloud"
		}
	},
	"workspaceLayout": {
		"appsDir": "apps",
		"libsDir": "libs"
	}
}
