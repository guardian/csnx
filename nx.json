{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		".eslintrc.json": "*",
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		"tools/nx-plugins/**/*": "*"
	},
	"npmScope": "csnx",
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"]
		}
	},
	"tasksRunnerOptions": {
		"default": {
			"options": {
				"accessToken": "OTdkYmFjMTYtNGFhNS00MjM2LTg0OGMtZmNhNGZiMmU2YTM5fHJlYWQtd3JpdGU=",
				"cacheableOperations": [
					"build",
					"lint",
					"test",
					"e2e",
					"build-storybook"
				]
			},
			"runner": "@nrwl/nx-cloud"
		}
	},
	"workspaceLayout": {
		"appsDir": "apps",
		"libsDir": "libs"
	},
	"generators": {
		"@nrwl/react": {
			"application": {
				"style": "@emotion/styled",
				"linter": "eslint",
				"babel": true
			},
			"component": {
				"style": "@emotion/styled"
			},
			"library": {
				"style": "@emotion/styled",
				"linter": "eslint"
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		],
		"test": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		],
		"e2e": [
			{
				"target": "build",
				"projects": "self"
			},
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	}
}
