{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"noImplicitReturns": true,
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"strict": true,
		"target": "esnext",
		"resolveJsonModule": true
	}
}
