{
	"extends": "../../tsconfig/tsconfig.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"lib": ["ES2019", "DOM"],
		"module": "commonjs",
		"noEmit": true,
		"types": ["node", "cypress"],
		"skipLibCheck": true,
		"strict": true,
		"paths": {
			"common/*": ["../static/src/javascripts/projects/common/*"],
			"lib/*": ["../static/src/javascripts/lib/*"],
			"types/*": ["../static/src/javascripts/types/*"]
		},
		"sourceMap": false
	},
	"include": ["**/*.ts", "../static/src/javascripts/types/global.d.ts"]
}
