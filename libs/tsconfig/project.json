{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/tsconfig",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/web:rollup",
			"outputs": ["{options.outputPath}"],
			"options": {
				"rollupConfig": "rollup.config.js",
				"project": "libs/tsconfig/package.json",
				"entryFile": "libs/tsconfig/src/index.ts",
				"outputPath": "dist/libs/tsconfig",
				"tsConfig": "libs/tsconfig/tsconfig.lib.json",
				"assets": [
					"libs/tsconfig/readme.md",
					"libs/tsconfig/CHANGELOG.md",
					"libs/tsconfig/tsconfig.json"
				],
				"format": ["esm", "cjs"]
			}
		}
	},
	"tags": []
}
