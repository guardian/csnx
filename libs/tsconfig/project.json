{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/tsconfig",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"outputs": ["dist/libs/tsconfig"],
			"options": {
				"commands": [
					"rm -rf dist/libs/tsconfig",
					"mkdir -p dist/libs/tsconfig",
					"cp libs/tsconfig/* dist/libs/tsconfig",
					"rm dist/libs/tsconfig/project.json"
				],
				"parallel": false
			}
		}
	},
	"tags": []
}
